<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="100.0" prefWidth="250.0" stylesheets="@../UI.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bo.roman.tinnitus.ui.controller.AttenuatorController">
	<children>
		<ButtonBar prefHeight="100.0" prefWidth="250.0">
			<buttons>
				<Button id="settings" fx:id="settings" alignment="TOP_LEFT" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#settingsAction" text="Settings" />
				<ToggleButton id="run" fx:id="run" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#runAction" text="ToggleButton" />
			</buttons>
		</ButtonBar>
	</children>
</HBox>
